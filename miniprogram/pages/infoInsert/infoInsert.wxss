/* pages/chooseWay/infoInsert.wxss */
.add-li{
    margin-top: 30rpx;
    margin-bottom: 10rpx;
    font-size: 30rpx;
    border-radius: 35rpx;
}
.create-li{
  margin-top: 30rpx;
  margin-left: 50rpx;
}

.round-click{
      height: 120rpx;
      width: 120rpx;
      background-color: #107C41;
      border-radius: 100%;
      position: fixed;
      bottom: 50rpx;
      right: 20rpx;
      display: flex;
      align-items: center;
      justify-content: center;
      z-index: 9;
  }
  .round-click navigator{
      font-size: 27rpx;
      max-width: 80rpx;
      color: #fff;
      text-align: center;
  }
  .product-item {
    position: relative;
    width: 100vw;
    border-bottom: 2rpx solid #E9E9E9;
    box-sizing: border-box;
    background: #fff;
    z-index: 999;
  }

  .movablearea{
    height: 120rpx;
    width: calc(100vw - 120rpx);
  }

  .movableview{
    height: 120rpx;
    width: 100vw;
    background: #fff;
    z-index: 999;
  }

  .delete-btn {
    position: absolute;
    top: 0;
    bottom: 0;
    right: 0; 
    width: 120rpx;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #FFFFFF;
    line-height: 120rpx;
    z-index: 1;
    background: #E66671;
    text-align: center;
  }
  
